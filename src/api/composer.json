{
  "name": "gonzalo123/grafanads",
  "type": "project",
  "license": "MIT",
  "description": "Grafana Datasource",
  "keywords": [],
  "require": {
    "php": ">=7.1.3",
    "kissifrot/php-ixr": "^1.8",
    "laravel/lumen-framework": "5.8.*",
    "swiftmailer/swiftmailer": "^6.0",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public"
  },
  "config": {
    "process-timeout": 0,
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
